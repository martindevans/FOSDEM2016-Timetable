      <pentabarf:title>Emulating the Nintendo 3DS</pentabarf:title>
      <pentabarf:subtitle>Status of the Citra project</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
