      <uid>3762@FOSDEM16@pentabarf.org</uid>
      <pentabarf:event-id>3762</pentabarf:event-id>
      <pentabarf:event-slug>tarantool_an_in_memory_nosql_database_and_execution_grid</pentabarf:event-slug>
